<h1>Add a Gig Here</h1>

<table>
  <%= form_tag (gigs_url) do %>
  <tr>
    <td>
      <%= label_tag :date, "Date: (YEAR-MO-DY-HR:MN)" %>
      <%= text_field_tag :date %>
    </td>
    <td>
      <%= label_tag :venue, "Venue:" %>
      <%= text_field_tag :venue %>
    </td>
    <td>
      <%= label_tag :cost, "$" %>
      <%= text_field_tag :cost %>
    </td>
    <td>
      <%= submit_tag "Submit" %>
    </td>
  </tr>
  <% end %>
</table>

<p><%= link_to "Concert Calendar", gigs_url %></p>
<p><%= link_to "Songs List", songs_url %></p>